<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <!--Maps login page to loginServlet servlet-->
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>loginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>homeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>profile</servlet-name>
    <servlet-class>profileServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>logoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>course</servlet-name>
    <servlet-class>courseServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>dropClass</servlet-name>
    <servlet-class>dropClassServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>homework</servlet-name>
    <servlet-class>homeworkServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>exam</servlet-name>
    <servlet-class>examServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>addAssignment</servlet-name>
    <servlet-class>addAssignmentServlet</servlet-class>
  </servlet>

  <!--Maps login page to /login url-->
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>home</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>profile</servlet-name>
    <url-pattern>profile</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>logout</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>course</servlet-name>
    <url-pattern>course</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>dropClass</servlet-name>
    <url-pattern>drop</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>homework</servlet-name>
    <url-pattern>homework</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>exam</servlet-name>
    <url-pattern>exam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>addAssignment</servlet-name>
    <url-pattern>addAssignment</url-pattern>
  </servlet-mapping>
</web-app>
